{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env", ".env.production"],
  "ui": "tui",
  "tasks": {
    "gen": {
      "dependsOn": ["^gen"]
    },
    "lint": {
      "dependsOn": ["gen"]
    },
    "format": {
      "dependsOn": ["gen"]
    },
    "typecheck": {
      "dependsOn": ["gen", "^typecheck"]
    },
    "dev": {
      "dependsOn": ["gen"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["gen", "^build"]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "migrate:dev": {},
    "migrate:prod": {},
    "deploy": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
